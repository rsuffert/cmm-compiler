func int main(int argc) {
    int array[sum(1,1) * 2];

    array[0] = self(4) / 2;
    array[1] = max(10,20);
    array[2] = 50;
    array[4] = (sum(argc,43) + 10 * 20) / 2;
}

func int sum(int a, int b) {
    int res;
    res = a + b;
    return res;
}

func int self(int a) {
    int res;
    res = a;
    return res;
}

func int max(int a, int b) {
    int max;
    if (a > b) {
        max = a;
    } else {
        max = b;
    }
    return max;
}